#include <stdio.h>

int main() {
    int numbers[5],i,min;
    printf("Enter 5 numbers: ");
    for (i = 0; i < 5; i++)
    {
    scanf("%d", &numbers[i]);
    }
    min = numbers[0];
    for (i = 1; i < 5; i++)
     {
        if (numbers[i] < min)
         {
            min = numbers[i];
        }
    }
    printf("\nThe smallest number is: %d\n", min);

    return 0;
}

output:
Enter 5 numbers: 7 2 9 5 8

The smallest number is: 2
