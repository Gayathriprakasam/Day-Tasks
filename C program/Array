#include <stdio.h>

int main() {
    int numbers[5],i,max;
    printf("Enter 5 numbers: ");
    for (i = 0; i < 5; i++)
    {
    scanf("%d", &numbers[i]);
    }
    max = numbers[0];
    for (i = 1; i < 5; i++)
     {
        if (numbers[i] > max)
         {
            max = numbers[i];
        }
    }
    printf("\nThe largest number is: %d\n", max);

    return 0;
}


output:
Enter 5 numbers: 4
7
9
1
6

The largest number is: 9
